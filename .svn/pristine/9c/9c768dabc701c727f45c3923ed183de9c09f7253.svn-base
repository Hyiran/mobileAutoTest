package com.bmtc.task.domain;

import java.io.Serializable;
import java.util.Date;

/**
 * 测试任务实体类
 * @author Administrator
 *
 */
public class BMTCTask implements Serializable{

	/**
	 * 属性
	 */
	private static final long serialVersionUID = 1L;
	// 测试任务ID
	private Long taskId;
	// 测试任务名称
	private String taskName;
	// 所属产品机构id
	private Long deptId;
	// 所属产品机构name
	private String deptName;
	// 所属批次的ID
	private Long batchId;
	// 所属批次的名称
	private String batchName;
	// 所属批次的库名
	private String batchSvnPath;
	// SVN路径
	private String svnPath;
	// 测试任务语言类型：1，Android；2，IOS
	private Long deviceType;
	// 创建人ID
	private Long createdId;
	// 状态
	private Long status;
	// 创建时间
	private Date gmtCreate;
	// 修改时间
	private Date gmtModified;
	
	/**
	 * 构造
	 */
	public BMTCTask() {
		super();
	}
	/**
	 * get&set方法
	 */
	public Long getTaskId() {
		return taskId;
	}
	public void setTaskId(Long taskId) {
		this.taskId = taskId;
	}
	public String getTaskName() {
		return taskName;
	}
	public void setTaskName(String taskName) {
		this.taskName = taskName;
	}
	public Long getDeptId() {
		return deptId;
	}
	public void setDeptId(Long deptId) {
		this.deptId = deptId;
	}
	public Long getCreatedId() {
		return createdId;
	}
	public void setCreatedId(Long createdId) {
		this.createdId = createdId;
	}
	public Long getStatus() {
		return status;
	}
	public void setStatus(Long status) {
		this.status = status;
	}
	public Date getGmtCreate() {
		return gmtCreate;
	}
	public void setGmtCreate(Date gmtCreate) {
		this.gmtCreate = gmtCreate;
	}
	public Date getGmtModified() {
		return gmtModified;
	}
	public void setGmtModified(Date gmtModified) {
		this.gmtModified = gmtModified;
	}
	public String getSvnPath() {
		return svnPath;
	}
	public void setSvnPath(String svnPath) {
		this.svnPath = svnPath;
	}
	public Long getDeviceType() {
		return deviceType;
	}
	public void setDeviceType(Long deviceType) {
		this.deviceType = deviceType;
	}
	public String getDeptName() {
		return deptName;
	}
	public void setDeptName(String deptName) {
		this.deptName = deptName;
	}
	public Long getBatchId() {
		return batchId;
	}
	public void setBatchId(Long batchId) {
		this.batchId = batchId;
	}
	public String getBatchName() {
		return batchName;
	}
	public void setBatchName(String batchName) {
		this.batchName = batchName;
	}
	public String getBatchSvnPath() {
		return batchSvnPath;
	}
	public void setBatchSvnPath(String batchSvnPath) {
		this.batchSvnPath = batchSvnPath;
	}
	/**
	 * toString
	 */
	@Override
	public String toString() {
		return "BMTCTask [taskId=" + taskId + ", taskName=" + taskName
				+ ", deptId=" + deptId + ", deptName=" + deptName
				+ ", batchId=" + batchId + ", batchName=" + batchName
				+ ", svnPath=" + svnPath + ", deviceType=" + deviceType
				+ ", createdId=" + createdId + ", status=" + status
				+ ", gmtCreate=" + gmtCreate + ", gmtModified=" + gmtModified + "]";
	}
}