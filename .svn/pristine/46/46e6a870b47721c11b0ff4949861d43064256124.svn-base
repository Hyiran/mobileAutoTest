package com.bmtc.svn.domain;

/**
 * SVN root用户信息类，用于获取url对应的svn的全路径
 * @author lpf7161
 *
 */
public class SvnRootUser {
	/**
	 * svn用户名
	 */
	private String svnUserName;
	
	/**
	 * svn密码
	 */
	private String svnPassword;
	
	/**
	 * svn仓库名
	 */
	private String svnRepoName;
	
	/**
	 * svn仓库路径
	 */
	private String svnRepoPath;
	
	/**
	 * svn仓库目录或文件名称
	 */
	private String svnItem;
	
	/**
	 * svn仓库url
	 */
	private String svnRepoUrl;
	
	/**
	 * svn树形列表中path
	 */
	private String path;

	public String getSvnUserName() {
		return svnUserName;
	}

	public void setSvnUserName(String svnUserName) {
		this.svnUserName = svnUserName;
	}

	public String getSvnPassword() {
		return svnPassword;
	}

	public void setSvnPassword(String svnPassword) {
		this.svnPassword = svnPassword;
	}

	public String getSvnRepoName() {
		return svnRepoName;
	}

	public void setSvnRepoName(String svnRepoName) {
		this.svnRepoName = svnRepoName;
	}

	public String getSvnRepoPath() {
		return svnRepoPath;
	}

	public void setSvnRepoPath(String svnRepoPath) {
		this.svnRepoPath = svnRepoPath;
	}

	public String getSvnRepoUrl() {
		return svnRepoUrl;
	}

	public void setSvnRepoUrl(String svnRepoUrl) {
		this.svnRepoUrl = svnRepoUrl;
	}
	
	public String getSvnItem() {
		return svnItem;
	}

	public void setSvnItem(String svnItem) {
		this.svnItem = svnItem;
	}

	public String getPath() {
		return path;
	}

	public void setPath(String path) {
		this.path = path;
	}

	@Override
	public String toString() {
		return "SvnRootUser [svnUserName=" + svnUserName + ", svnPassword="
				+ svnPassword + ", svnRepoName=" + svnRepoName
				+ ", svnRepoPath=" + svnRepoPath + ", svnItem=" + svnItem
				+ ", svnRepoUrl=" + svnRepoUrl + ", path=" + path + "]";
	}
	
	public SvnRootUser(String svnUserName, String svnPassword,
			String svnRepoName, String svnRepoPath, String svnItem,
			String svnRepoUrl, String path) {
		super();
		this.svnUserName = svnUserName;
		this.svnPassword = svnPassword;
		this.svnRepoName = svnRepoName;
		this.svnRepoPath = svnRepoPath;
		this.svnItem = svnItem;
		this.svnRepoUrl = svnRepoUrl;
		this.path = path;
	}

	public SvnRootUser() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	
}
