<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content animated fadeInRight">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox-content">
					<form class="form-horizontal m-t" id="svnUserRightPassTestForm">
						<div class="form-group">
							<label class="col-sm-3 control-label">SVN仓库名：</label>
							<div class="col-sm-8">
								<input id="svnRepoName" name="svnRepoName" th:value="${svnInfo.svnRepoName}"
									class="form-control" type="text" readonly="readonly" >
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label">SVN仓库URL：</label>
							<div class="col-sm-8">
								<input id="svnRepoUrl" name="svnRepoUrl" th:value="${svnInfo.svnRepoUrl}"
									class="form-control" type="text" readonly="readonly" >
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label">SVN用户名：</label>
							<div class="col-sm-8">
								<input id="svnUserName" name="svnUserName" class="form-control"
									type="text" th:value="${svnInfo.svnUserName}" readonly="readonly">
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label">SVN路径：</label>
							<div class="col-sm-8">
								<input id="svnPath" name="svnPath" class="form-control"
									type="text" th:value="${svnInfo.svnPath}" readonly="readonly">
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label">SVN权限：</label>
							<div class="col-sm-8">
								<input id="svnUserAuthz" name="svnUserAuthz" class="form-control"
									type="text" th:value="${svnInfo.svnUserAuthz}" readonly="readonly">
							</div>
						</div>
						<div class="form-group">
							<div class="col-sm-8 col-sm-offset-3">
								<button type="submit" class="btn btn-primary">提交</button>
							</div>
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/svn/svnUserRight/passTest.js">
	</script>
</body>
</html>
